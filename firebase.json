{
  "firestore": {
    "indexes": "firestore.indexes.json",
    "rules": "firestore.rules"
  },
  "hosting": {
    "headers": [
      {
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache"
          }
        ],
        "source": "/service-worker.js"
      }
    ],
    "ignore": ["**/.*", "**/node_modules/**"],
    "public": "dist/pwa"
  },
  "storage": {
    "rules": "storage.rules"
  }
}
